<h1>新規案件投稿</h1>
<%= form_with model: @project, local:true do |f| %>
  <h4>イメージ画像</h4>
   <%= f.attachment_field :image %>
  <h4>報酬</h4>
  <%= f.text_field :price %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>説明文</h4>
  <%= f.text_area :details %>
  <h4>タグ</h4>
  <%= f.text_field :tag_ids, class: "form-control", id:'project-tags',placeholder: "タグをつける。複数つけるには','で区切ってください。" %>

  <%= f.submit '投稿' %>
<% end %>
