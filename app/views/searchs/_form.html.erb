<%= form_with url: searchs_search_path,  method: :get, data: {remote: false} do %>
  <%= text_field_tag 'content' %>
  <% if company_signed_in? %>
    <%= select_tag 'model', options_for_select({ "インフルエンサー" => "influencer", "タグ" => "tag", "案件" => "project" }) %>
  <% else %>
    <%= select_tag 'model', options_for_select({ "タグ" => "tag", "案件" => "project" }) %>
  <% end %>
  <%= submit_tag '検索',class: "search-btn" %>
<% end %>
