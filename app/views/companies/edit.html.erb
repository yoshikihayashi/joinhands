<h2>プロフィール編集</h2>
<%= form_with model:@company, local:true do |f| %>
  <label for="inputName">会社名</label>
  <%= f.text_field :company_name, autofocus: true, id:"inputName", placeholder:"会社名"%>
  <label for="inputName">事業内容</label>
  <%= f.text_field :business_detail, autofocus: true, id:"inputName", placeholder:"事業内容"%>
  <label for="inputName">従業員数</label>
  <%= f.text_field :employee_number, autofocus: true, id:"inputName", placeholder:"従業員数"%>
  <%= f.submit "変更を保存" %>
<% end %>